<div class="content">
    <h1><%= title %> </h1>
  
    <% if (book_list.length > 0) { %>
      <ul>
          <% book_list.forEach(function(book) { %>
              <li>
                <a href="<%= book._id %>"><%= book.title %></a>
                <%= book.title %>
              
              </li>
          <% }); %>
      </ul>
  <% } else { %>
      <p>There are no books.</p>
  <% } %>


  